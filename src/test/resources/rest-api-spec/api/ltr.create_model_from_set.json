      "paths": ["/_ltr/{store}/_featureset/{name}/_createmodel","/_ltr/_featureset/{name}/_createmodel"],
      "parts": {
        "store": {
          "type": "string",
          "required": false,
          "description": "The store name"
        },
        "name": {
          "type": "string",
          "required": true,
          "description": "The featureset name"
        }
      },
      "params": {
        "routing": {
          "type" : "string",
          "description" : "Specific routing value"
        }
      }
    },
    "body": {
      "description" : "Definition of the model: name, type and model",
      "required": "true"
    }
  }
}

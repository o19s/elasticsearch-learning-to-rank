{
  "ltr.list_feature": {
    "methods" : ["GET"],
    "url": {
      "paths": ["/_ltr/_feature", "/_ltr/{store}/_feature"],
      "parts": {
        "store": {
          "type": "string",
          "required": false,
          "description": "The store name"
        }
      },
      "params": {
        "prefix": {
          "type": "string",
          "required": false,
          "description": "Filter elements by name prefix"
        },
        "size": {
          "type": "integer",
          "required": false,
          "description": "Page size"
        },
        "from": {
          "type": "integer",
          "required": false,
          "description": "Page results"
        }
      }
    },
    "body": null
  }
}
